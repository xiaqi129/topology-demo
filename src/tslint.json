{
    "defaultSeverity": "error",
    "extends": "../tslint.json",
    "rules": {
        "no-trailing-whitespace": true,
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}